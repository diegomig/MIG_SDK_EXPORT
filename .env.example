# ========================================
# MIG TOPOLOGY SDK - EXAMPLE CONFIG (SAFE)
# ========================================
#
# Copy to `.env` and fill with your own values:
#   cp .env.example .env
#
# IMPORTANT:
# - Do NOT commit `.env` (it contains secrets).
# - Use GitHub Actions secrets / system env vars in CI/production.
#

# PostgreSQL
POSTGRES_PASSWORD=change_me
POSTGRES_PORT=5432
DATABASE_URL=postgresql://mig_topology_user:change_me@localhost:5432/mig_topology?options=-c%20client_encoding=utf8

# Redis
REDIS_PORT=6379
REDIS_URL=redis://localhost:6379

# Logging
RUST_LOG=info
RUST_BACKTRACE=1

# RPC providers
SDK_RPC_HTTP_URLS=["https://arb-mainnet.g.alchemy.com/v2/YOUR_KEY","https://arbitrum-mainnet.infura.io/v3/YOUR_KEY"]
SDK_RPC_WS_URLS=["wss://arb-mainnet.g.alchemy.com/v2/YOUR_KEY","wss://arbitrum-mainnet.infura.io/v3/YOUR_KEY"]

# Optional API keys
SDK_ARBISCAN_API_KEY=
SDK_GRAPH_API_KEY=
SDK_BITQUERY_API_KEY=
SDK_BITQUERY_SECRET=
SDK_BITQUERY_ACCESS_TOKEN=
SDK_INFURA_API_KEY=

